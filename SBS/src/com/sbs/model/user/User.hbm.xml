<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sbs.model.user">
	<class name="User" table="USER">
		<id name="userID" column="USERID">
			<generator class="assigned" />
		</id>		
		<property name="firstname">
			<column name="FIRSTNAME" length="30" not-null="false" />
		</property>
		<property name="lastname">
			<column name="LASTNAME" length="30" not-null="false" />
		</property>
		
		<property name="address">
			<column name="ADDRESS" length="60" not-null="false"/>
		</property>
		
		<property name="contact">
			<column name="CONTACT" length="12" not-null="false"/>
		</property>
		
		<property name="emailid">
			<column name="EMAILID" length="30" not-null="false"/>
		</property>
		
		<property name="DOB">
			<column name="DOB" not-null="false"/>
		</property>
		
		<property name="idtype">
			<column name="IDTYPE" length="20" not-null="false"/>
		</property>
		
		<property name="idNo">
			<column name="IDNO" length="20" not-null="false"/>
		</property>
		<property name="secureQ1">
			<column name="SECUREQ1" length="40" not-null="false"/>
		</property>
		<property name="secureQ2">
			<column name="SECUREQ2" length="40" not-null="false"/>
		</property>
		
		<property name="secureQ3">
			<column name="SECUREQ3" length="40" not-null="false"/>
		</property>
		
		<property name="secureA1">
			<column name="SECUREA1" length="40" not-null="false"/>
		</property>
		
		<property name="secureA2">
			<column name="SECUREA2" length="40" not-null="false"/>
		</property>
		
		<property name="secureA3">
			<column name="SECUREA3" length="40" not-null="false"/>
		</property>
		
		<property name="roleID">
			<column name="ROLEID"  not-null="false"/>
		</property>
		
		<property name="deptID">
			<column name="DEPTID"  not-null="false"/>
		</property>
		
		
	</class>
</hibernate-mapping>